{

"AWSEBDockerrunversion":2,
"containerDefinitions":[
   {
       "name": "client",
       "image": "asharanees/multi-client",
       "hostname": "client",
       "essential":false,
       "memory": 128
    },

    {
        "name": "worker",
        "image": "asharanees/multi-worker",
        "hostname": "worker",
        "essential":false,
        "memory": 128
    },

    {
        "name": "server",
        "image": "asharanees/multi-server",
        "hostname": "api",
        "essential":false,
        "memory": 128
    },

    {
        "name": "nginx",
        "image": "asharanees/multi-nginx",
        "hostname": "nginx",
        "essential":true,
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client","server"],
        
        "memory": 128
    }

]

}